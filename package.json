{
  "name": "kleur",
  "version": "3.0.3",
  "repository": "lukeed/kleur",
  "description": "The fastest Node.js library for formatting terminal text with ANSI colors~!",
  "module": "index.mjs",
  "main": "index.js",
  "license": "MIT",
  "types": "kleur.d.ts",
  "exports": {
    ".": {
      "import": "./index.mjs",
      "require": "./index.js"
    }
  },
  "files": [
    "*.d.ts",
    "index.js",
    "index.mjs"
  ],
  "author": {
    "name": "Luke Edwards",
    "email": "luke.edwards05@gmail.com",
    "url": "https://lukeed.com"
  },
  "scripts": {
    "build": "bundt index.mjs --main",
    "test": "uvu -r esm -i utils"
  },
  "engines": {
    "node": ">=10"
  },
  "keywords": [
    "ansi",
    "cli",
    "color",
    "colors",
    "console",
    "terminal"
  ],
  "devDependencies": {
    "bundt": "1.0.0",
    "esm": "3.2.25",
    "uvu": "0.0.11"
  }
}
